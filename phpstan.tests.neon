includes:
	- phpstan.base.neon

parameters:
	level: 1

	paths:
		- tests

	ignoreErrors:
		# Intentional, tests reporting
		- message: '#^Missing \@throws Tests\\Modette\\Exceptions\\Fixtures\\ExampleDomainException annotation$#'
		  path: %currentWorkingDirectory%/tests/Unit/Check/CheckedExceptionTest.php
		  count: 1
