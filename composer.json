{
	"name": "modette/exceptions",
	"description": "Base exceptions designed for static analysis and easy usage",
	"keywords": [
		"modette",
		"exceptions",
		"phpstan"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/modette/exceptions",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "^7.2.0"
	},
	"require-dev": {
		"ninjify/qa": "^0.10.0",
		"pepakriz/phpstan-exception-rules": "^0.10.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^8.5.0"
	},
	"autoload": {
		"psr-4": {
			"Modette\\Exceptions\\": "src"
		}
	},
	"autoload-dev": {
	  "psr-4": {
		"Tests\\Modette\\Exceptions\\": "tests"
	  }
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.1.x-dev"
		}
	}
}
